<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="top.warmj.dao.FileDao">

    <resultMap type="File" id="FileResultMap">
        <id column="file_box_id" property="fileBoxId"/>
        <result column="number_order" property="numberOrder"/>
        <result column="add_time" property="addTime"/>
        <result column="last_time" property="lastTime"/>
    </resultMap>

    <select id="getFile" parameterType="int" resultMap="FileResultMap">
    SELECT
        id,
        file_box_id,
        number_order,
        path,
        type,
        add_time,
        last_time
    FROM
        file
    WHERE
        file_box_id = #{fileBoxId}
    </select>
</mapper>
